<%= form_for(@article) do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :content %>
  <%= f.text_field :content %>
  <%= f.label :author %>
  <%= f.text_field :author %>
  <%= f.label :published %>
  <%= f.date_select :published, start_year: 1800, end_year: Time.now.year %>
  <%= f.submit %>
<% end %>

<%= link_to "Back to articles", articles_path %>
